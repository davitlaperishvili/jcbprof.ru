<div class="box webdigifyblog bottom-to-top hb-animate-element">
    <div class=" dl_container">
      <div class="row">
        <div class="dl_theme">
          <h2 class="section_title">
            <img src="catalog/view/theme/Gobazar/image/images/news.png" alt="">
            {{ heading_title }}
          </h2>
        </div>
        <div class="box-content">
          <div class="box-product owl-carousel blogcarousel" id="blog-carousel">
		{% for blog in blogs %} 
            <div class="blog-item">
              <div class="product-block">
                <div class="blog-left">
                  <div class="blog-image">
                    <a title="Click to view Read More " href="{{ blog.href }}">
                      <img src="{{ blog.image }}" alt="{{ heading_title }}" title="{{ heading_title }}" class="img-thumbnail" />
                    </a>
                  </div>
                </div>

                <div class="blog-right">
                  <div class="date-time"><i class="fa fa-calendar"></i> {{ blog.date_added }}</div>
                  <div class="view-blog">
                    <h4><a href="{{ blog.href }}">{{ blog.title }}</a></h4>
                    <div class="blog-desc"> {{ blog.description }} </div>
                    <div class="read-more">
                      <a href="{{ blog.href }}"> <i class="fa fa-link"></i> {{ text_read_more }}</a>
                    </div>
                  </div>
                </div>
              </div>
			  </div>
		{% endfor %} 


